{
	"name": "Mermaid",
	"version": "2.4.0-alpha",
	"author": [
			"James Hong Kong",
			"Tyler Gibson"
	],
	"url": "https://github.com/SemanticMediaWiki/Mermaid/",
	"descriptionmsg": "Mediawiki Extension enabling Mermaid.js diagrams at runtime",
	"license-name": "GPL-2.0-or-later",
	"type": "parserhook",
	"requires": {
		"MediaWiki": ">= 1.33"
	},
	"ExtensionMessagesFiles": {
		"MermaidMagic": "Mermaid.magic.php"
	},
	"MessagesDirs": {
		"Mermaid": [
			"i18n"
		]
	},
	"AutoloadClasses": {
		"Mermaid\\Hooks":"includes/Hooks.php",
		"Mermaid\\MermaidParserFunction": "includes/MermaidParserFunction.php"
	},
	"ResourceModules": {
		"ext.mermaid": {
			"scripts": [
				"modules/ext.mermaid.js",
				"lib/mermaid.min.js"
			],
			"dependencies": [
				"mediawiki.api"
			],
			"styles": [
				"styles/ext.mermaid.css"
			],
			"messages": [
				"mermaid-desc"
			],
			"targets": [
				"desktop",
				"mobile"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteExtPath": "Mermaid"
	},
	"ExtensionMessagesFiles": {
		"MermaidMagic": "i18n/Mermaid.i18n.php"
	},
	"Hooks" : {
		"ParserFirstCallInit" : "Mermaid\\Hooks::onParserFirstCallInit",
		"OutputPageParserOutput" : "Mermaid\\Hooks::onOutputPageParserOutput"
	},
	"config": {
		"Theme": "forest"
	},
	"manifest_version": 2
}
